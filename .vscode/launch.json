  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "NPM Break",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run-script", "start-brk"],
      "sourceMaps": true,
      "port": 9229
    },
    {
      "type": "node",
      "request": "launch",
      "name": "NPM Start",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["start"],
      "port": 9229
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}\\dist\\index.js"
    }
  ]
}
